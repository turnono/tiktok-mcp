{
  "name": "@turnono/tiktok-mcp",
  "version": "0.2.0",
  "description": "Backend-agnostic MCP server for TikTok analysis",
  "license": "MIT",
  "author": "turnono",
  "homepage": "https://github.com/turnono/tiktok-mcp",
  "type": "module",
  "bin": {
    "tiktok-mcp-server": "build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
